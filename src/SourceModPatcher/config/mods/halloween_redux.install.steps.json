[
  {
    "$type": "copy_files",
    "name": "halloween_redux_copy_files",
    "description": "Copy files to $(sourcemod_folder)",
    "files": [
      {
        "src": "$(data_sourcemod_dir)/gameinfo.txt",
        "dest": "$(sourcemod_dir)/gameinfo.txt"
      },
      {
        "src": "$(data_sourcemod_dir)/maps/hoodrd_01a_l_0.lmp",
        "dest": "$(sourcemod_dir)/maps/hoodrd_01a_l_0.lmp"
      },
      {
        "src": "$(data_sourcemod_dir)/maps/hoodrd_01b_l_0.lmp",
        "dest": "$(sourcemod_dir)/maps/hoodrd_01b_l_0.lmp"
      },
      {
        "src": "$(data_sourcemod_dir)/maps/hoodrd_02a_l_0.lmp",
        "dest": "$(sourcemod_dir)/maps/hoodrd_02a_l_0.lmp"
      },
      {
        "src": "$(data_sourcemod_dir)/resource/Halloween HL2 PN_english.txt",
        "dest": "$(sourcemod_dir)/resource/Halloween HL2 PN_english.txt"
      }
    ]
  }
]
