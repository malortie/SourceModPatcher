[
  {
    "$type": "copy_files",
    "name": "halloween_copy_files",
    "description": "Copy files to $(sourcemod_folder)",
    "files": [
      {
        "src": "$(data_sourcemod_dir)/gameinfo.txt",
        "dest": "$(sourcemod_dir)/gameinfo.txt"
      },
      {
        "src": "$(data_sourcemod_dir)/maps/hood01_final_l_0.lmp",
        "dest": "$(sourcemod_dir)/maps/hood01_final_l_0.lmp"
      },
      {
        "src": "$(data_sourcemod_dir)/materials/console/background01_widescreen.vtf",
        "dest": "$(sourcemod_dir)/materials/console/background01_widescreen.vtf"
      },
      {
        "src": "$(data_sourcemod_dir)/resource/Halloween_english.txt",
        "dest": "$(sourcemod_dir)/resource/Halloween_english.txt"
      }
    ]
  }
]
